<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE module PUBLIC "-//Google Inc.//DTD Google Web Toolkit 2.7.0//EN" "http://gwtproject.org/2.7.0/gwt-module.dtd">
<module rename-to='demogwt'>
	<!-- Inherit the core Web Toolkit stuff. -->
	<inherits name="com.google.gwt.user.User" />
	<inherits name="com.google.gwt.user.History"/>
	
	<!-- SmartGWT specific modules that are located in smartgwt.jar or smartgwt-skins.jar -->
	<inherits name="com.smartgwt.SmartGwtNoScript"/>	
    <inherits name="com.smartclient.theme.enterprise.EnterpriseResources"/>
    
    <!-- For SmartClient console -->
    <inherits name="com.smartgwt.tools.SmartGwtTools"/>
    
    <!-- Source paths relative to module file (Demo.gwt.xml) -->
	<source path="app" />
	
	<!-- The app starts here -->
	<entry-point class="com.demo.gwt.app.App"></entry-point>
	
</module>